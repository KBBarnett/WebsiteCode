<!-- FAQ -->
<script>
  // Add click event to each question and arrow
  const faqItems = document.querySelectorAll('.faq-item');

  faqItems.forEach((item) => {
    const question = item.querySelector('.faq-question');
    const arrow = item.querySelector('.faq-arrow');
    const answer = item.querySelector('.faq-answer');

    const toggleAnswer = () => {
      // Check if the answer is visible
      const isAnswerVisible = answer.classList.contains('faq-answer-visible');
      
      // Toggle the class based on the visibility
      answer.classList.toggle('faq-answer-visible', !isAnswerVisible);
    };

    question.addEventListener('click', toggleAnswer);
    arrow.addEventListener('click', toggleAnswer);
  });
</script>

</body> <!-- End Body -->
